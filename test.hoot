fn fib(x) {
    if x <= 1 {
        return 1;
    }
    else {
        return fib(x - 1) + fib(x - 2);
    }
}

fn fib_iter(x) {
    let a = 1;
    let b = 1;

    let i = 0;
    while i < x {
        let tmp = a + b;
        a = b;
        b = tmp;

        i = i + 1;
    }

    return a;
}

fn main() {
    let i = 0;
    while i < 50 {
        let recurse = fib(i);
        let iter = fib_iter(i);

        if recurse == iter {
            println(iter);
        }
        else {
            return 0 - 1;
        }

        i = i + 1;
    }
    return 0;
}
